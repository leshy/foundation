    "size-limit": "^8.2.4",
    "tslib": "^2.5.2",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "dts-cli": "^2.0.3",
    "lodash": "^4.17.21",
